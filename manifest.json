{
  "manifest_version": 2,

  "name": "HTML5 Video Speed Control",
  "description": "Speed controls for html5 video.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "<all_urls>"],
      "css": ["style.css"],
      "js": ["speedcontrol.js"]
    }
  ]
}
