{
  "manifest_version": 2,

  "name": "HTML5 Video Speed Control",
  "description": "Speed controls for html5 video.",
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "css": ["style.css"],
      "js": ["jquery-2.0.3.min.js", "speedcontrol.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["jquery-2.0.3.min.js", "jquery-2.0.3.min.map"]
}
